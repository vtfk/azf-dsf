{
  "name": "azf-dsf",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "func start",
    "test": "jest && standard",
    "test:lint": "standard",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "fix": "standard --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtfk/azf-dsf.git"
  },
  "keywords": [
    "vtfk",
    "Vestfold og Telemark fylkeskommune",
    "dsf",
    "Azure Functions"
  ],
  "author": "Mats Andreassen <matsand@live.no> (https://github.com/matsand)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtfk/azf-dsf/issues"
  },
  "homepage": "https://github.com/vtfk/azf-dsf#readme",
  "dependencies": {
    "@vtfk/logger": "2.5.1",
    "jsonwebtoken": "8.5.1",
    "node-dsf": "3.1.0"
  },
  "devDependencies": {
    "jest": "26.6.3",
    "standard": "16.0.3"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+.(spec|test).[jt]s?(x)"
    ]
  }
}
