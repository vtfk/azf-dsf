{
  "name": "azf-dsf",
  "version": "2.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "func start",
    "test": "jest && standard",
    "test:lint": "standard",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "fix": "standard --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "publish:prod": "npm run test && func azure functionapp publish prod-func-dsf-api-v1",
    "publish:test": "npm run test && func azure functionapp publish test-func-dsf-api-v1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtfk/azf-dsf.git"
  },
  "keywords": [
    "vtfk",
    "Vestfold og Telemark fylkeskommune",
    "dsf",
    "Azure Functions"
  ],
  "author": "Mats Andreassen <matsand@live.no> (https://github.com/matsand)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtfk/azf-dsf/issues"
  },
  "homepage": "https://github.com/vtfk/azf-dsf#readme",
  "dependencies": {
    "@vtfk/e18": "2.1.5",
    "@vtfk/logger": "4.1.2",
    "capitalize": "2.0.4",
    "jsonwebtoken": "9.0.0",
    "node-dsf": "3.1.0"
  },
  "devDependencies": {
    "jest": "27.5.1",
    "standard": "16.0.4"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)",
      "**/?(*.)+.(spec|test).[jt]s?(x)"
    ]
  }
}